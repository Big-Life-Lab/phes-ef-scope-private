@article{HuangShuQiong_DS_2016,
  title = {Evaluation on Quality of Notifiable Communicable Disease Reporting in {{Hubei}}, 2011-2015.},
  author = {{Huang ShuQiong} and {Cai Jing} and {Yang WenWen}},
  year = {2016},
  journal = {Disease Surveillance},
  volume = {31},
  number = {9},
  pages = {786--790},
  publisher = {Editorial Board of Disease Surveillance},
  address = {Beijing, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20173002942&site=ehost-live},
  abstract = {Objective: To evaluate the quality of notifiable communicable disease reporting by the medical institutions in Hubei, and provide evidence for the improvement of the disease surveillance system. Methods: The field survey data of communicable disease network direct reporting during 2011-2015 were collected to conduct a statistical analysis with TOPSIS, together with the network direct reporting data. The reporting rate, timely reporting rate, the reporting completeness rate, the reporting accuracy rate and reporting consistent rate were calculated. Results: The overall reporting quality of communicable diseases increased with year, except the decline of the reporting consistent rate in recent three years. The reporting quality of the medical institutions at county level was lower than those of the medical institutions at township and prefecture level. Conclusion: Great progress has been achieved in communicable diseases reporting in Hubei, but problems still exist, it is suggested to strengthen the management of communicable disease reporting in medical institutions at all levels, especially the medical institutions at county level, to improve the quality of notifiable communicable disease reporting.},
  keywords = {No DOI found,PRQ1_evaluation_studies_2015+}
}

@article{HuangYao_JoTM_2018,
  title = {Evaluation of the Sensitivity of Tuberculosis Surveillance System in {{Nanshan}} District of {{Shenzhen}} City Using "Case Stock Study".},
  author = {{Huang Yao} and {Liu ShengYuan} and {Fan YuZheng} and {He Juan} and {Zhu MinMin} and {Zhong Tao} and {Huang JinCheng} and {Yang YaRei}},
  year = {2018},
  journal = {Journal of Tropical Medicine (Guangzhou)},
  volume = {18},
  number = {10},
  pages = {1351--1354},
  publisher = {Sun Yat-sen Medical Science of Sun Yat-sen University},
  address = {Guangzhou, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20193048151&site=ehost-live},
  abstract = {Objective: To investigate the missing report rate of tuberculosis (TB) and under-registration rate of TB in Nanshan district of Shenzhen city in 2016 by inventory study. Methods: Collected TB related records registered from outpatient department, inpatient department, laboratory, radiology, pathology and prevention department in all medical institutions for the qualification of TB diagnosis and treatment (that is, the ability to perform X-ray chest examination). The missing report rate was calculated by comparing above records with data from national infectious disease reporting system; the under-registration rate was calculated by comparing diagnosis information from designated TB institutions with data from the TB surveillance system. Results: 2 322 suspected and diagnosed cases of TB were treated in medical institutions of Nanshan district in 2016. 16 cases of TB were failed to report and the total missing report rate was 0.69\%. The missing report rate of designated TB institutions, public medical institutions and private medical institutions was 0.79\% (6/760), 1.72\% (37/2 151) and 38.10\% (24/63), respectively; 692 diagnosed cases of TB were found in medical institutions of Nanshan district, of which 513 cases were reported and registered, 179 cases were missed, and the under-registration rate was 25.87\%. The primary reason for the under-registration is that the patients choose to go to Shenzhen Tuberculosis Hospital - The Third People's Hospital of Shenzhen for treatment. Conclusions: All kinds of medical institutions in Nanshan district were missing reports, so the process of card reporting in medical institutions should be standardized and the training of medical staff should be strengthened to improve the quality of tuberculosis reporting. Under-registration rate was higher in this area, so effective measures should be taken to reduce leakage rate.},
  keywords = {No DOI found,PRQ1_evaluation_studies_2015+}
}

@article{Hunduma_PAMJOH_2021,
  title = {Public Health Surveillance System Evaluation: Case of Rabies Surveillance in {{Burayu Town}}, {{Oromiya}}, {{Ethiopia}}, 2019},
  author = {Hunduma, F. and Leulseged, B.},
  year = {2021},
  journal = {Pan African Medical Journal One Health},
  volume = {4},
  doi = {10.11604/pamj-oh.2021.4.5.22048},
  url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85147495763&doi=10.11604%2fpamj-oh.2021.4.5.22048&partnerID=40&md5=6d4d5671f89acb12961315fc64c3f72c},
  abstract = {Introduction: public health surveillance is ongoing, systematic collection, analysis, interpretation, and dissemination of data regarding a health-related event for use in public health action to reduce morbidity and mortality and to improve health. In Ethiopia, 20 diseases and conditions are selected to be included in routine surveillance that reported immediately or weekly. Rabies is among immediately reportable disease. This study aims to evaluate the existing surveillance system of rabies in Burayu Town, Oromiya regional state. Methods: this surveillance system evaluation was conducted in Burayu Town, Oromiya region by cross-sectional study design. Data was collected from Burayu Health Office, three Health centers, and five Health Posts using standard questioner prepared for public health surveillance system evaluation from January 15-30, 2019. Data were analyzed by MS. Excel. Results: Burayu town has 302,543 population; all are at risk of rabies. The rabies surveillance system is being conducted regularly and consistently in the town. It captured 452 cases over the last four and half years, among them 267 (59.1\%) were confirmed to be positive by laboratory investigation. It was useful, flexible, simple, sensitive, and stable. However, resources are poor, supervision and feedback are also intermittent and not regular. Epidemic preparedness and response team is not established at health facilities and planning for the surveillance is not practiced. Conclusion: the surveillance system is functioning persistently, but regular supervision and feedback, availing case definitions and guidelines should be ameliorated. A written plan preparation by health facilities regarding the surveillance system, document keeping, resource planning and data analysis requires improvement. {\copyright} Fufa Hunduma et al.},
  keywords = {PRQ1_evaluation_studies_2015+}
}

@article{Husereau_BM_2022,
  title = {Consolidated {{Health Economic Evaluation Reporting Standards}} 2022 ({{CHEERS}} 2022) Statement: Updated Reporting Guidance for Health Economic Evaluations},
  shorttitle = {Consolidated {{Health Economic Evaluation Reporting Standards}} 2022 ({{CHEERS}} 2022) Statement},
  author = {Husereau, Don and Drummond, Michael and Augustovski, Federico and {de Bekker-Grob}, Esther and Briggs, Andrew H. and Carswell, Chris and Caulley, Lisa and Chaiyakunapruk, Nathorn and Greenberg, Dan and Loder, Elizabeth and Mauskopf, Josephine and Mullins, C. Daniel and Petrou, Stavros and Pwu, Raoh-Fang and Staniszewska, Sophie and {on behalf of CHEERS 2022 ISPOR Good Research Practices Task Force}},
  year = {2022},
  month = jan,
  journal = {BMC Medicine},
  volume = {20},
  number = {1},
  pages = {23},
  issn = {1741-7015},
  doi = {10.1186/s12916-021-02204-0},
  url = {https://doi.org/10.1186/s12916-021-02204-0},
  urldate = {2024-11-15},
  abstract = {Health economic evaluations are comparative analyses of alternative courses of action in terms of their costs and consequences. The Consolidated Health Economic Evaluation Reporting Standards (CHEERS) statement, published in 2013, was created to ensure health economic evaluations are identifiable, interpretable, and useful for decision making. It was intended as guidance to help authors report accurately which health interventions were being compared and in what context, how the evaluation was undertaken, what the findings were, and other details that may aid readers and reviewers in interpretation and use of the study. The new CHEERS 2022 statement replaces previous CHEERS reporting guidance. It reflects the need for guidance that can be more easily applied to all types of health economic evaluation, new methods and developments in the field, as well as the increased role of stakeholder involvement including patients and the public. It is also broadly applicable to any form of intervention intended to improve the health of individuals or the population, whether simple or complex, and without regard to context (such as health care, public health, education, social care, etc). This summary article presents the new CHEERS 2022 28-item checklist and recommendations for each item. The CHEERS 2022 statement is primarily intended for researchers reporting economic evaluations for peer reviewed journals as well as the peer reviewers and editors assessing them for publication. However, we anticipate familiarity with reporting requirements will be useful for analysts when planning studies. It may also be useful for health technology assessment bodies seeking guidance on reporting, as there is an increasing emphasis on transparency in decision making.},
  keywords = {PRQ1_reference_searching},
  file = {files/53759/Husereau et al. - 2022 - Consolidated Health Economic Evaluation Reporting .pdf;files/53758/s12916-021-02204-0.html}
}
