@article{IlesanmiO.S._IJoID_2016,
  title = {Evaluation of {{Ebola}} Virus Disease Surveillance System in {{Tonkolili}} District, {{Sierra Leone-2015}}},
  author = {{Ilesanmi O.S.} and {Fawole O.} and {Nguku P.} and {Oladimeji A.}},
