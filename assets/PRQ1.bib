@article{DoolanK._IJoMS_2016,
  title = {Hiv Surveillance in {{Ireland}}; an Evaluation of Timeliness and Sensitivity, 2012-2013},
  author = {{Doolan K.} and {Giese C.} and {Igoe D.} and {Cullen G.} and {Moran J.} and {De Gascun C.} and {O'Flanagan D.}},
  year = {2016},
  journal = {Irish Journal of Medical Science},
  volume = {185},
  number = {12 Supplement 1},
  pages = {S556},
  publisher = {Springer London},
  address = {Netherlands},
  doi = {10.1007/s11845-016-1510-y},
  abstract = {HIV is a lifelong infection of public health concern requiring timely and accurate surveillance. In light of concerns that there was a degree of laboratory underreporting of HIV which increased in 2013, we undertook an evaluation of the system, focusing on sensitivity and timeliness. We estimated underreporting by analysing new HIV diagnoses that were not notified in 2013 because they did not fit the nationally agreed criteria for notification (i.e. two serology positives on two separate samples). To assess timeliness, we used the national Computerised Infectious Disease Reporting System (CIDR) and National Virus Reference Laboratory (NVRL) 2012/2013 data to calculate median time intervals in HIV surveillance steps, from HIV first sample confirmation to completion of enhanced information in CIDR. In 2013, 71 new HIV diagnoses did not meet the criteria for notification to CIDR (sensitivity 82\%): 23 had one serology positive only and 48 had a second sample tested for viral loads which were undetectable. Of 665 notifications, the interval between NVRL first diagnosis and form completion varied from 20 (14-117; 11-239) to 49 (27-125; 12-252) days by DPH area. Overall, 81\% of notifications had enhanced information available 6 months after diagnosis. We recommend that reporting procedures are amended to notify on confirmation of HIV on the first sample at NVRL. The timeliness of the HIV surveillance system was in line with the American surveillance standard (66\% reported within 6 months of diagnosis). Reliable analysis of HIV trends can be undertaken within 6 months of diagnosis.},
  keywords = {PRQ1_evaluation_studies_2015+}
}

@article{DoyleM._C_2022,
  title = {Evaluation of a {{System}} for {{Real-Time Surveillance}} of {{Suicide}} in {{England}}},
  author = {{Doyle M.} and {Ainsworth P.} and {Boul S.} and {Lee D.}},
  year = {2022},
  journal = {Crisis},
  number = {(Doyle) Department of Nursing, School of Human and Health Sciences, University of Huddersfield, United Kingdom(Ainsworth, Lee) Barnsley Metropolitan Borough Council, Barnsley, United Kingdom(Boul) NHS England and Improvement in South Yorkshire and Bassetl},
  publisher = {NLM (Medline)},
  address = {Canada},
  doi = {10.1027/0227-5910/a000874},
  abstract = {Background: Access to timely data on suicide is crucial to support suicide prevention. A real-time suicide surveillance (RTSS) system enables public health teams and allied agencies to review information following suicides promptly and take action quickly. Aim(s): The aim of this paper was to report on an evaluation of an RTSS system in South Yorkshire, England. Method(s): The system was reviewed, and outcome data were analyzed for 2019 and 2020 based on recorded suspected suicides, a stakeholder focus group, evaluation of postvention bereavement service outcomes, and lessons learned. Result(s): The benefits of RTSS included rapid response to emerging trends, identifying clusters, effective bereavement support, information to inform measures to mitigate risk, and supporting evaluation of interventions. The challenges faced included limited resources, data quality, consistency across places, and linkages with coronial processes. Limitation(s): This was an evaluation of one RTSS system based on routinely collected data covering one area, South Yorkshire, and hence some data limitations, and direct comparison with other services or against a control group was not possible. Conclusion(s): The RTSS system has led to better support for suicidal people and a responsive, timely, and effective service for those bereaved by suicide, all of which are likely to lead to enhanced well-being and community resilience.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/37778/Doyle M. et al. - 2022 - Evaluation of a System for Real-Time Surveillance .pdf}
}
