@article{fan2014,
  title = {Evaluation of Outbreak Detection Performance Using Multi-Stream Syndromic Surveillance for Influenza-like Illness in Rural {{Hubei Province}}, {{China}}: A Temporal Simulation Model Based on Healthcare-Seeking Behaviors.},
  author = {Fan, Yunzhou and Wang, Ying and Jiang, Hongbo and Yang, Wenwen and Yu, Miao and Yan, Weirong and Diwan, Vinod K and Xu, Biao and Dong, Hengjin and Palm, Lars and Nie, Shaofa},
  year = {2014},
