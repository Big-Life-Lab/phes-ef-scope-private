@article{PreetiPadda_IJoCH_2018,
  title = {Surveillance, Evaluation of {{Programmatic Management}} of {{Drug Resistant Tuberculosis}} ({{PMDT}}) at {{DR-TB Centre}}, {{NITRD}}, {{New Delhi}}.},
