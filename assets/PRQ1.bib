@article{Chen_Eapp_2019,
  title = {Assessment of the Monitoring and Evaluation System of a Population-Based Program: {{Theory-driven}} Evaluation Approach.},
  author = {Chen, Huey T and Morosanu, Liliana and {Powell-Threets}, Kia and Lian, Brad and Turner, Nannette},
  year = {2019},
  journal = {Evaluation and program planning},
  volume = {77},
  number = {7801727, eob},
  pages = {101719},
  address = {England},
  doi = {10.1016/j.evalprogplan.2019.101719},
  abstract = {The purpose of this study was to assess the performance of a monitoring and evaluation system (M\&E) for a population-based program and to identify factors influencing its performance. The state health department provided a grant, guide, and training for health districts to implement the program and report quarterly and annually to the M\&E. Some health districts did not meet this requirement. We also found that the M\&E did not adequately capture the actual work of the districts in communities as related to the work plan. We identified several factors that negatively affected the performances of the M\&E. For example, the state and health districts did not have a shared vision on how to develop and implement the program, and what to report to the M\&E. We proposed strategies that could improve the timeliness and content of reports to the M\&E, as well as its utilization in the future. Copyright {\copyright} 2019. Published by Elsevier Ltd.},
  keywords = {PRQ1_evaluation_studies_2015+}
}

@article{ChenHuiHuan_MPM_2016,
  title = {Quality Evaluate of Influenza Surveillance in {{Zhaoqing}} City from 2013 to 2015.},
  author = {{Chen HuiHuan} and {Mai Wei} and {Deng Hong}},
  year = {2016},
  journal = {Modern Preventive Medicine},
  volume = {43},
  number = {24},
  pages = {4516--4518},
  publisher = {Magazine House of Modern Preventive Medicine},
  address = {Chengdu, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20173082244&site=ehost-live},
  abstract = {Objective: The aim of this study was to understand the situation of the influenza surveillance in Zhaoqing, in order to further improve the quality of this work. Methods: The data was obtained from a series of management systems included the influenza surveillance, the epidemic outbreak monitoring and the etiology monitoring of the network laboratory and from the reports of the cases of influenza like illness (ILI) as well as the samples of collecting and delivering and so on. The data was used to evaluate the basis of The National Evaluation Schemes of the Quality for the Influenza Surveillance (2012 edition). Results: From 2013 to 2015, the index of the quality for influenza surveillance in Zhaoqing was good and only 33\% (7/21) positive specimens from the epidemic outbreak monitoring were performed with the viral isolation. There were only 59.46\% (198/333) positive specimens were performed with the identification of subtypes from 2013 to 2014. The index of the timeliness of strain inspection from chicken embryos separation was the weakest point from 2014 to 2015. The ratio of timely reporting of ILI in the municipal level fell from 93.46\% to 84.62\% and was lower than that in the national level (X2=11.469, P{$<$}0.001). There were two weeks' specimens that did not conform to the quantitative requirement and six specimens failed to be timely inspected. Conclusion: The quality of the work for influenza surveillance management was good. But the work for the viral isolation and delivery of specimens from the epidemic outbreak monitoring, the report of ILI and the collection and inspection of specimens was insufficient. We should take measures to improve it timely.},
  keywords = {No DOI found,PRQ1_evaluation_studies_2015+}
}

@article{ChenW.-S._AoTM_2019,
  title = {Evaluation of Manual and Electronic Healthcare-Associated Infections Surveillance: {{A}} Multi-Center Study with 21 Tertiary General Hospitals in {{China}}},
  author = {{Chen W.-S.} and {Zhang W.-H.} and {Li Z.-J.} and {Yang Y.} and {Chen F.} and {Ge X.-S.} and {Wang T.-R.} and {Fang P.} and {Feng C.-Y.} and {Liu J.} and {Liu S.-S.} and {Pan H.-X.} and {Zhu T.-L.} and {Tian Y.-Y.} and {Wang W.-Y.} and {Xing H.} and {Yao J.} and {Yuan Y.-M.} and {Jiang P.} and {Tang H.-P.} and {Zhou J.} and {Zang J.-C.} and {Lu S.} and {Huang H.-P.} and {Lei X.-H.} and {Huang B.-H.} and {Wang S.-H.} and {Huang F.-Y.} and {Tao H.-Y.} and {Zhang Y.-X.} and {Liu B.} and {Li H.-F.} and {Li S.-Q.} and {Hu B.-J.} and {Liu Y.}},
  year = {2019},
  journal = {Annals of Translational Medicine},
  volume = {7},
  number = {18},
  pages = {80},
  publisher = {AME Publishing Company (E-mail: info@amepc.org)},
  address = {Hong Kong},
  doi = {10.21037/atm.2019.08.80},
  url = {http://atm.amegroups.com/},
  abstract = {Background: Healthcare-associated infections (HAIs) are still a major health threats worldwide. Traditional surveillance methods involving manual surveillance by infection control practitioners (ICPs) for data collection processes are laborious, inefficient, and generate data of variable quality. In this study, we sought to evaluate the impact of surveillance and interaction platform system (SIPS) for HAIs surveillance compared to manual survey in tertiary general hospitals. Method(s): A large multi-center study including 21 tertiary general hospitals and 63 wards were performed to evaluate the impact of electronic SIPS for HAIs. Result(s): We collected 4,098 consecutive patients and found that the hospitals installed with SIPS significantly increased work efficiency of ICPs achieving satisfactory diagnostic performance of HAIs with 0.73 for sensitivity, 0.81 for specificity and 0.81 area under the curve (AUC). However, there were significant heterogeneity own to regions, time of SIPS installation, departments and sample size. Conclusion(s): SIPS significantly improved ICPs efficiency and HAIs monitoring effectiveness, but there were shortcomings such as untimely maintenance and high cost.Copyright {\copyright} Annals of Translational Medicine. All rights reserved.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/40853/Chen W.-S. et al. - 2019 - Evaluation of manual and electronic healthcare-ass.pdf}
}

@article{ChenXiangYang_MPM_2016,
  title = {Health Economic Evaluation of the Monitoring System of Leprosy Suspicious Symptoms in {{Wenzhou}}, 2012-2015.},
  author = {{Chen XiangYang} and {Qin Xia}},
  year = {2016},
  journal = {Modern Preventive Medicine},
  volume = {43},
  number = {17},
  pages = {3154--3156},
  publisher = {Magazine House of Modern Preventive Medicine},
  address = {Chengdu, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20173010011&site=ehost-live},
  abstract = {Objective: To evaluate the economic interest of the monitoring system of leprosy suspicious symptoms in Wenzhou, and to provide a scientific basis for the utilization of health economic facilities. Methods: Using data from the project of the monitoring system of leprosy suspicious symptoms during 2012-2015, the health economic evaluation of the monitoring system was conducted by the cost-effectiveness analysis, cost-utility analysis and cost-benefit analysis. Results: 314318 yuan had been invested in the system during 2012-2015. The economic evaluation suggested 34924.22 yuan would be spent once diagnosing one leprosy case, 974.39 yuan would be spent once shortening one month of the delayed diagnosis, 41837.9 yuan would be spent once avoiding one case of misdiagnosis, and 150616.5 yuan would be spent once avoiding one case of grade II disabilities. The average time of delayed diagnosis was shortened by 34.35 months, and the average frequency of misdiagnosis was reduced by 0.8 times. 2 cases of grade II disabilities were avoided, and DALY of 72.64 years was retrieved. The social total value of 3860777.5 yuan was saved, the net benefit was 3546459.5 yuan, and the cost-benefit ratio was 12.28. Conclusion: The monitoring system of leprosy suspicious symptom has a better cost-benefit ratio in controlling the epidemic of leprosy, which achieves a sustainable development.},
  keywords = {No DOI found,PRQ1_evaluation_studies_2015+},
  note = {Emma Brown (2023-11-25 08:29:51)(Select): Get error or article does not exist message on multiple platforms.;}
}

@article{ChenXiaoYing_DS_2008,
  title = {Evaluation of Report Quality of Infectious Diseases in {{Baoji}} City, {{Shaanxi}} Province, 2007.},
  author = {{Chen XiaoYing}},
  year = {2008},
  journal = {Disease Surveillance},
  volume = {23},
  number = {10},
  publisher = {Editorial Board of Disease Surveillance},
  address = {Beijing, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20083318009&site=ehost-live},
  abstract = {The study was conducted to evaluate the quality of report of infectious diseases after the implementation of the direct network report as well as the performance of this network report in Baoji city, Shaanxi province [China], and discuss the major factors influencing its quality. Data extracted from reports of class B and C infectious diseases in the information management system of disease surveillance, a subsystem of China Disease Prevention and Control Information System, were entered into the Excel database for statistical analysis. Reports of infectious diseases mainly came from general hospitals, accounting for 75.38\% of the total number. The direct report rate was 81.02\% among the medical institutions at all levels with 80.72\% in general hospitals and 85.62\% in township health centres. No non-verified report, missing report, duplicated report nor misreport were found. The rate of timely epidemic report during the year reached 96.62\%, an increase of 4.49\% compared with 2006. The rate of timely verification was 98.79\%, an increased of 1.51\% compared with 2006. Timeliness of report is thus, a major factor to influence the quality of reports in Baoji. Since the report quality varied by different counties and districts, the standardization and management of direct network report should further be improved.}
}

@article{Cherif_SpF_2018,
  title = {[{{Evaluation}} of Epidemiological Monitoring of Post-Vaccination Adverse Reactions in {{Abidjan}}].},
  author = {Cherif, Djibril and N Guessan, Konan Elvis Nezi and Coulibaly, Daouda and {Zengbe-Acray}, Petronille and Ekra, Kouadio Daniel and Traore, Amadou and Tiembre, Issiaka},
  year = {2018},
  journal = {Sante publique (Vandoeuvre-les-Nancy, France)},
  volume = {30},
  number = {3},
  pages = {411--417},
  address = {France},
  doi = {10.3917/spub.183.0411},
  abstract = {INTRODUCTION: This study was designed to evaluate the adverse events following immunization (AEFI) monitoring system in Abidjan health districts., METHODS: This cross-sectional study focused on activities conducted between 1st January and 31st December 2015 in the nine health districts of Abidjan. Data were collected by means of a questionnaire based on the model of World Health Organization evaluation forms. These questionnaires were administered to personnel in charge of epidemiological surveillance in the health districts and the personnel of district hospitals and health centres., RESULTS: The AEFI monitoring system, apart from its simplicity and flexibility, remains fairly ineffective due to the relatively low acceptability by all health workers involved. Only 4\% of healthcare personnel respondents participate in this activity; 55\% of health districts reported cases of AEFI and 25\% of health centres reported cases of adverse events. The low acceptability makes this system less sensitive and ineffective for health policy decision-making designed to reduce adverse events following immunization., CONCLUSION: Improved performance of the AEFI monitoring system requires better training of health workers, better data processing equipment and improved community participation.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/40979/Cherif et al. - 2018 - [Evaluation of epidemiological monitoring of post-.pdf}
}

@article{Chimsimbe_TPAmj_2022,
  title = {An Evaluation of the Notifiable Disease Surveillance System in {{Chegutu District}}, {{Zimbabwe}}, 2020: A Cross-Sectional Study.},
  author = {Chimsimbe, Memory and Mucheto, Pride and Govha, Emmanuel and Chadambuka, Addmore and Karakadzai, Mujinga and Juru, Tsitsi Patience and Gombe, Notion Tafara and Tshimanga, Mufuta},
  year = {2022},
  journal = {The Pan African medical journal},
  volume = {41},
  number = {101517926},
  pages = {215},
  address = {Uganda},
  doi = {10.11604/pamj.2022.41.215.33712},
  abstract = {Introduction: in 2018-2019 Chegutu District had one notification form Tally 1 (T1) that was completed instead of seven for detected notifiable diseases. Different figures of cholera were reported through weekly rapid disease notification system with 106 patients and Notifiable Diseases Surveillance System (NDSS) with 111 patients, causing data discrepancy. We evaluated the NDSS to determine reasons for underperformance and data discrepancy., Methods: we conducted descriptive cross-sectional study using updated centres for disease control and prevention guidelines for surveillance system evaluation. We recruited forty-six health workers. Interviewer-administered questionnaires and checklists were used to collect data on reasons for underperformance, reasons for data discrepancy, knowledge of NDSS, surveillance system attributes and usefulness. Epi InfoTM7 generated frequencies, proportions, and means. Likert scale was used to assess health worker knowledge., Results: of the forty-six health workers, 34 (78\%) had fair knowledge of NDSS. The reason for system underperformance was lack of training in NDSS 42 (91\%). Data discrepancy was attributed to typographical mistakes made during data entry on WhatsApp platform 32 (70\%). Eighty per cent (37) were willing to complete T1 forms. Six participants who were timed took ten minutes to complete T1 forms. Among 17 health facilities, only three had fifteen T1 forms that were adequate to notify first five cases in an outbreak. Notifiable diseases surveillance system data was used for planning health education 28 (68\%)., Conclusion: the NDSS was unstable due to health workers' inadequate knowledge and unavailability of T1 forms. Notifiable diseases surveillance system was found to be simple, acceptable, and useful. We recommended NDSS training of health workers. Copyright: Memory Chimsimbe et al.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/37926/Chimsimbe et al. - 2022 - An evaluation of the notifiable disease surveillan.pdf}
}

