@article{TomasiS.E._AJoRaCCM_2017,
  title = {Niosh Health Hazard Evaluation Program as a Sentinel Surveillance System for Novel Occupational Lung Diseases},
  author = {{Tomasi S.E.} and {Fechter-Leggett E.D.} and {Cummings K.J.} and {Bailey R.} and {Nett R.J.}},
  year = {2017},
  journal = {American Journal of Respiratory and Critical Care Medicine},
  volume = {195},
  number = {(Tomasi) Epidemic Intelligence Service, Center for Surveillance, Epidemiology and Laboratory Services, Atlanta, GA, United States(Fechter-Leggett, Cummings, Bailey, Nett) NIOSH, Morgantown, WV, United States},
  publisher = {American Thoracic Society},
  address = {Netherlands},
  doi = {10.1164/ajrccm-conference.2017.B58},
  url = {http://www.atsjournals.org/doi/pdf/10.1164/ajrccm-conference.2017.195.1_MeetingAbstracts.A3868},
  abstract = {Rationale: As industries and technologies continue to evolve, identifying novel occupational lung diseases remains an import aspect of occupational safety and health. Recognizing preventable and novel sentinel occupational lung diseases provides warning signals to occupational health professionals that can lead to new worker safety research and recommendations. No systematic Federal notification system exists to identify novel occupational lung diseases. The Occupational Safety and Health (OSH) Act of 1970 authorizes the National Institute for Occupational Safety and Health (NIOSH) to respond to written requests from three employees, management, or a union representative to provide the service of performing worksite hazard evaluations through the Health Hazard Evaluation (HHE) Program. We aimed to determine if the HHE Program could also serve as a sentinel surveillance system to identify or characterize novel occupational lung diseases. Method(s): Using the methods and criteria outlined by the Centers for Disease Control and Prevention's guidelines for evaluating a public health surveillance system to the extent possible, the HHE program was evaluated as a sentinel surveillance system for novel occupational lung diseases. Through stakeholder interviews, the HHE program was assessed for simplicity, flexibility, data quality, acceptability, sensitivity, predictive value positive, representativeness, timeliness, and stability. These criteria were judged using a scoring system of 1 to 4 representing the perceived effectiveness of the HHE Program as a surveillance system. Result(s): The NIOSH HHE Program has used field investigations to characterize several newly recognized occupational lung diseases including obliterative bronchiolitis in microwave popcorn, flavoring, and coffee workers; flock interstitial lung disease in flock manufacturing workers; and hypersensitivity pneumonitis in workers exposed to metalworking fluids. The HHE Program's strengths include data quality, predictive value positive, and stability (Table). Sensitivity and representativeness require the most development for the HHE Program to function as a sentinel surveillance system. Conclusion(s): In addition to the HHE Program's primary role as a service, with its high data quality, strong predictive value positive, and stability, it could also function as a sentinel surveillance system to identify novel occupational lung diseases. However, the sensitivity and representativeness of the HHE Program is limited by low worker and employer awareness of the Program and perceived negative consequences to making an HHE request. Improving employer, employee, and health care provider awareness about the HHE Program's benefits would enhance the program's sensitivity and representativeness as a surveillance system for novel occupational lung diseases; however, additional resources could be required to investigate additional requests. (Table Presnted).},
  keywords = {PRQ1_evaluation_studies_2015+}
}

@article{Tosti_Ph_2015,
  title = {Assessment of Timeliness, Representativeness and Quality of Data Reported to {{Italy}}'s National Integrated Surveillance System for Acute Viral Hepatitis ({{SEIEVA}}).},
  author = {Tosti, M E and Longhi, S and {de Waure}, C and Mele, A and Franco, E and Ricciardi, W and Filia, A},
  year = {2015},
  journal = {Public health},
  volume = {129},
  number = {5},
  pages = {561--8},
  address = {Netherlands},
  doi = {10.1016/j.puhe.2015.02.015},
  abstract = {OBJECTIVES: Periodic assessment of surveillance systems is recommended to verify whether they are appropriately monitoring the public health problem under surveillance. The aim of this study was to evaluate timeliness, data quality and representativeness of data reported to the Italian Integrated Epidemiological System for Acute Viral Hepatitis (SEIEVA)., STUDY DESIGN: Cross-sectional analysis of surveillance data., METHODS: Quantitative indicators were used to evaluate representativeness of reported cases, data quality, and timeliness between surveillance steps, for reports of acute viral hepatitis cases with date of onset of symptoms from 2009 to 2012 (N = 4516)., RESULTS: Representativeness was 75\%. Over 95\% of records reported information on age, sex, city of residence, risk factors for hepatitis A and vaccination status. Information on risk factors for hepatitis B and C were reported less consistently (83\%), as was information on early outcome (60\%). Wide delays were found between surveillance steps., CONCLUSIONS: The system collects high quality data on acute viral hepatitis cases in Italy. Timeliness was found to be the main limit and needs to be improved by optimizing web-based reporting procedures, increasing communication with participating centres, improving feedback and increasing dissemination of surveillance results. The study highlights the importance of reporting timeliness to detect outbreaks of acute viral hepatitis. Copyright {\copyright} 2015 The Royal Society for Public Health. Published by Elsevier Ltd. All rights reserved.},
  file = {files/37748/Tosti et al. - 2015 - Assessment of timeliness, representativeness and q.pdf}
}

@article{Tozan_TAjotmah_2023,
  title = {A {{Methodological Framework}} for {{Economic Evaluation}} of {{Operational Response}} to {{Vector-Borne Diseases Based}} on {{Early Warning Systems}}.},
  author = {Tozan, Yesim and Odhiambo Sewe, Maquines and Kim, Sooyoung and Rocklov, Joacim},
  year = {2023},
  journal = {The American journal of tropical medicine and hygiene},
  volume = {108},
  number = {3},
  pages = {627--633},
  address = {United States},
  doi = {10.4269/ajtmh.22-0471},
  abstract = {Despite significant advances in improving the predictive models for vector-borne diseases, only a few countries have integrated an early warning system (EWS) with predictive and response capabilities into their disease surveillance systems. The limited understanding of forecast performance and uncertainties by decision-makers is one of the primary factors that precludes its operationalization in preparedness and response planning. Further, predictive models exhibit a decrease in forecast skill with longer lead times, a trade-off between forecast accuracy and timeliness and effectiveness of action. This study presents a methodological framework to evaluate the economic value of EWS-triggered responses from the health system perspective. Assuming an operational EWS in place, the framework makes explicit the trade-offs between forecast accuracy, timeliness of action, effectiveness of response, and costs, and uses the net benefit analysis, which measures the benefits of taking action minus the associated costs. Uncertainty in disease forecasts and other parameters is accounted for through probabilistic sensitivity analysis. The output is the probability distribution of the net benefit estimates at given forecast lead times. A non-negative net benefit and the probability of yielding such are considered a general signal that the EWS-triggered response at a given lead time is economically viable. In summary, the proposed framework translates uncertainties associated with disease forecasts and other parameters into decision uncertainty by quantifying the economic risk associated with operational response to vector-borne disease events of potential importance predicted by an EWS. The goal is to facilitate a more informed and transparent public health decision-making under uncertainty.},
  keywords = {PRQ1_evaluation_studies_2015+}
}
