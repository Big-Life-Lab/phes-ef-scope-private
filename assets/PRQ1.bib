@article{MaZuFang_DS_2009,
  title = {Quality Evaluation of Network Reporting of Infectious Disease in {{Jinan}}, 2007.},
  author = {{Ma ZuFang} and {Shan Bing} and {Xu ShuHui}},
  year = {2009},
  journal = {Disease Surveillance},
  volume = {24},
  number = {1},
  publisher = {Editorial Board of Disease Surveillance},
  address = {Beijing, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20093131943&site=ehost-live},
  abstract = {The factors influencing the promptness of reporting infectious diseases in Jinan City, Shandong, China, and the measures to improve promptness were investigated. The report cards of infectious diseases reported by the Report and Management System of Disease Surveillance Information in 2007 were analysed. 33{\enspace}457 cases were reported in the city, with an average reporting time of 0.09 day. The medical institutions reported cases mostly during 08.30-11.00 or 14.00-17.00 h, accounting for 76.96\% of the total reports. Majority of the report verifications (52.18\%) were performed in the Center for Disease Control at 08.30-11.00 and 14.00-17.00 h. The promptness of infectious disease report was 99.80\% in 2007, and that of verification was 99.90\%. The highest promptness rate for reporting was 100\% in Zhangqiu City, and the lowest was 98.61\% in Zhangqing District. The highest promptness rate for verification was 100\% in Huaiyin District, Tianqiao District and Zhangqiu City, and the lowest was 99.21\% in Licheng District. It is concluded that network reporting of infectious disease is very timely and accurate. However, there are significant differences in the quality of reporting among counties. It is required to establish agents specifically responsible for network reporting, management of epidemics, and timely reporting and verification of report cards to promote report quality in Jinan City.}
}

@article{MbejeN.P._CI_2021,
  title = {Establishing and {{Evaluating Cancer Surveillance System}} in {{KwaZulu-Natal}}, {{South Africa}}},
  author = {{Mbeje N.P.} and {Ginindza T.G.} and {Jafta N.}},
  year = {2021},
  journal = {Cancer Informatics},
  volume = {20},
  number = {(Mbeje, Ginindza) Discipline of Public Health Medicine, School of Nursing and Public Health, University of KwaZulu-Natal, Durban, South Africa(Jafta) Discipline of Occupational and Environmental Health, School of Nursing and Public Health, University of K},
  publisher = {SAGE Publications Ltd},
  address = {United Kingdom},
  doi = {10.1177/11769351211029967},
  url = {https://journals.sagepub.com/home/cix},
  abstract = {Background: The increasing cancer burden remains a public health challenge. Quality and accurate population data is important to improve cancer control, screening, and treatment programmes for the sub-Saharan Africa region. Aim(s): The aim of this study was to establish hospital-based cancer surveillance system, thereby reporting the burden that cancer diagnosis and treatment place on 3 hospitals - an approach of health systems strengthening. Method(s): A hospital-based cancer surveillance was established in 3 public health facilities that provide oncology services in KwaZulu-Natal. An active method was used for finding cancer cases. The cancer surveillance database was evaluated according to the criteria recommended for cancer registries. Analyses of data included descriptive and crude incidence rates. Result(s): A total of 2307 newly diagnosed cancer cases were reported in 2018, with a majority from Inkosi Albert Luthuli Central hospital (65.3\%), followed by Greys hospital (30.8\%) and then Addington hospital (3.94\%). Most of the cancer cases were from the 2 major urban areas of the province (eThekwini and uMgungundlovu district). The most commonly diagnosed cancers from all combined 3 facilities for both sexes were breast, cervix, colorectal, Kaposi Sarcoma, and lung. Approximately half of the cancer cases had no staging, and 12.8\% of the cases were diagnosed at stage 4. The mostly prescribed treatments for the patients were radiotherapy and chemotherapy. Conclusion(s): Based on our hospital-based surveillance, cancer burden is high in the 3 facilities. Strengthening cancer screening and diagnostic policies and procedures that will allow expansion of accurate cancer surveillance system is essential in KwaZulu-Natal and South Africa as a whole.Copyright {\copyright} The Author(s) 2021.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/37934/Mbeje N.P. et al. - 2021 - Establishing and Evaluating Cancer Surveillance Sy.pdf}
}
