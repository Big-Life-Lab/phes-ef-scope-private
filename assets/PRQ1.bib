@article{GarzonRomero_IQ-EN_2018,
  title = {Evaluation of Epidemiological Field Research on Foodborne Disease Outbreaks, {{Bogot{\'a}}}, {{January}} 2016-{{October}} 2017.},
  author = {Garz{\'o}n Romero, F. E. and Prieto Alvarado, F. E. and Alarc{\'o}n Cruz, {\'A}. P.},
  year = {2018},
  journal = {Informe Quincenal - Epidemiol{\'o}gico Nacional},
  volume = {23},
  number = {12},
  pages = {134--163},
  publisher = {Ministerio de Salud},
  address = {Bogot{\'a}, Colombia},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20193008607&site=ehost-live},
  abstract = {Foodborne diseases (FBDs) caused by infectious or toxic agents are considered an event of public health interest. The World Health Organization (WHO) estimates that one in 10 people get sick annually due to FBDs, the risk being higher in children under 5 years of age. The objective of this study was to evaluate the outbreak response to FBDs in Bogot{\'a} Capital District, Colombia. This cross-sectional study was based on the evaluation of 60 outbreaks reported in Bogot{\'a} between January 2016 and October 2017. Compliance with surveillance indicators was assessed (notification, field investigation, sampling and identification of the aetiological agent). Indicators of research opportunity were observed in 86.5\%, notification and identification in {$<$}67\%, analysis of the situation prior to research in 40\%, and use of data collection instruments in 50\%. The survey was limited to a single time of consumption in 75.0\%, and with interview of healthy and sick people in 45\%. The source and mode of transmission were detected in 56.7\% and 76.0\%, respectively. The implementation of sanitary measures was observed in {$<$}50\%. It is necessary to strengthen the opportunity of identification, notification of outbreaks, the method of collection of laboratory samples, the implementation of situation analysis to action plans in the field, definition of data collection instruments, and the implementation of analytical phase to determine associated factors, with the purpose of formulating and implementing better prevention and control measures.},
  keywords = {No DOI found,PRQ1_evaluation_studies_2015+}
}

