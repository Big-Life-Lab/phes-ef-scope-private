@article{JohnsonK._STD_2020,
  title = {Congenital Syphilis Surveillance and Prevention System Evaluation-{{New York City}}, 2018},
  author = {{Johnson K.} and {Hennessy R.} and {Slutsker J.} and {Bushman D.}},
  year = {2020},
  journal = {Sexually Transmitted Diseases},
  volume = {47},
  number = {9 SUPPL 2},
  pages = {S67},
  publisher = {{Lippincott Williams and Wilkins}},
  address = {Netherlands},
  abstract = {Background: Untreated syphilis infection during pregnancy can cause severe outcomes, including stillbirth, preterm birth, and early infant death. Although preventable, congenital syphilis (CS) increased 186\% in New York City (NYC), from 7 cases in 2017 to 20 cases in 2018. The NYC CS surveillance and prevention system (the system) collects data on pregnant New Yorkers with untreated syphilis and pregnancy outcomes including CS. We evaluated the system to determine whether it captured necessary data to identify and facilitate treatment of pregnant New Yorkers with untreated syphilis and to identify areas for system improvement. Method(s): The director of surveillance for Sexually Transmitted Infections identified 8 NYC Department of Health and Mental Hygiene (DOHMH) CS stakeholders, including an analyst, researcher, data manager, director of case investigations, IT project manager, and congenital infections coordinator, with whom interviews were conducted. Using the 2001 CDC Guidelines for Evaluating Public Health Surveillance Systems, we analyzed NYC DOHMH 2018 syphilis surveillance data. Result(s): In 2018, DOHMH identified a total of 8,372 new syphilis cases. Among these cases, 99\% had an electronic laboratory report and 35\% had a paper or electronic provider report. Stakeholders indicated {\textasciitilde}50\% of female case reports were received without an indication of pregnancy status. Considerable time was therefore spent by 81 DOHMH staff to subsequently gather pregnancy information and determine that 100 (1.2\%) of the new syphilis cases were among pregnant New Yorkers with untreated syphilis, of whom 87 ultimately received treatment to prevent CS (sensitivity 87\%). Conclusion(s): Incomplete pregnancy data accompanying female syphilis case reports resulted in time-consuming investigations to ascertain pregnancy status which potentially delayed interventions. Facilitating inclusion of pregnancy status in the laboratory and provider syphilis reports and assigning specialized staff to prioritize CS prevention over other responsibilities could decrease personnel effort, time to maternal syphilis treatment, and save newborn lives.},
  keywords = {PRQ1_evaluation_studies_2015+}
}

