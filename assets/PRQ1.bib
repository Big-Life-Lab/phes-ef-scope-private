  title = {Assessment of the {{Hepatitis C Surveillance System}} in {{Henan}}, {{China}}: 20142016},
  author = {{Li J.} and {Shang X.} and {Liang Y.} and {Yang W.} and {Wang Z.} and {Ji H.}},
  year = {2018},
  journal = {BioMed Research International},
  volume = {2018},
  number = {(Li, Liang, Yang, Wang) STD and AIDS Prevention Institute, Henan Center for Disease Control and Prevention, Zhengzhou, Henan, China(Shang) AIDS Prevention Section, Erqi District Center for Disease Control and Prevention, Zhengzhou, Henan, China(Ji) Nation},
