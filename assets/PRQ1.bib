  address = {Beijing, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20113312190&site=ehost-live},
  abstract = {Objective: To evaluate the management and quality of communicable diseases network direct reporting in China (except Hong Kong and Macao SARs, Taiwan area) and find out the existing problems for the further improvement of reporting quality. Methods: The evaluation was conducted for the health administrations, centers disease control and prevention (CDCs) and hospital selected at all levels by multi stage stratified random samplings in 31 provinces (autonomous region, municipality) and Xinjiang Production and Construction Corps in China. Results: A total of 459 health institutions were investigated, and the data utilization rate was 100\%. The management model in all level health administrations and CDCs had been greatly improved, including financial support, supervision, inspection and evaluation, clinic registration standardization, data utilization and information dissemination as well as training and technical guidance. Generally the management of communicable disease reporting in eastern and central regions were better than that in western region. The overall underreporting rate of communicable diseases in the hospitals sampled was 5.47\%, the rate of timely reporting, the complete rate of paper cards, the consistent rate of paper cards and electronic cards was 94.84\%, 89.36\% and 70.23\% respectively. The reporting quality in provincial hospitals was not satisfied, and the reporting quality in eastern and central regions was significantly better than that in western region. Conclusion: The more standardized management of communicable diseases network direct reporting had been conducted and the reporting quality had been greatly improved in China. It is necessary for all health institutions to continue to strengthen the management of communicable diseases network direct reporting, especially for provincial hospitals. The reporting quality of communicable diseases in western region should be improved.},
  keywords = {No DOI found}
}

@article{LiuShiWei_DS_2011a,
  title = {Evaluation on Quality of Notifiable Communicable Diseases Network Direct Reporting and Discussion about Evaluation Indicators in {{China}}, 2009.},
