@article{ClickE.S._IJoTaLD_2012,
  title = {Evaluation of Integrated Registers for Tuberculosis and {{HIV}} Surveillance in Children, {{Ethiopia}}, 2007-2009},
  author = {{Click E.S.} and {Feleke B.} and {Pevzner E.} and {Fantu R.} and {Gadisa T.} and {Assefa D.} and {Melaku Z.} and {Cain K.} and {Menzies H.}},
