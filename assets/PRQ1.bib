@article{Hoinville_Pvm_2013,
  title = {Proposed Terms and Concepts for Describing and Evaluating Animal-Health Surveillance Systems.},
  author = {Hoinville, L J and Alban, L and Drewe, J A and Gibbens, J C and Gustafson, L and Hasler, B and Saegerman, C and Salman, M and Stark, K D C},
