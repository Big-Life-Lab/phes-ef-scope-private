@article{Streefkerk_Icahe_2016,
  title = {Internal and {{External Validation}} of a {{Computer-Assisted Surveillance System}} for {{Hospital-Acquired Infections}} in a 754-{{Bed General Hospital}} in the {{Netherlands}}.},
  author = {Streefkerk, H Roel A and Lede, Ivar O and Eriksson, John L V and Meijling, Marije G and {van der Hoeven}, Conrad P and Wille, Jan C and Hopmans, Titia E M and Friedrich, Alex W and Verbrugh, Henri A and Naiemi, Nashwan Al},
  year = {2016},
  journal = {Infection control and hospital epidemiology},
  volume = {37},
  number = {11},
  pages = {1355--1360},
  address = {United States},
  doi = {10.1017/ice.2016.159},
  abstract = {OBJECTIVE To evaluate a computer-assisted point-prevalence survey (CAPPS) for hospital-acquired infections (HAIs). DESIGN Validation cohort. SETTING A 754-bed teaching hospital in the Netherlands. METHODS For the internal validation of a CAPPS for HAIs, 2,526 patients were included. All patient records were retrospectively reviewed in depth by 2 infection control practitioners (ICPs) to determine which patients had suffered an HAI. Preventie van Ziekenhuisinfecties door Surveillance (PREZIES) criteria were used. Following this internal validation, 13 consecutive CAPPS were performed in a prospective study from January to March 2013 to determine weekly, monthly, and quarterly HAI point prevalence. Finally, a CAPPS was externally validated by PREZIES (Rijksinstituut voor Volksgezondheid en Milieu [RIVM], Bilthoven, Netherlands). In all evaluations, discrepancies were resolved by consensus. RESULTS In our series of CAPPS, 83\% of the patients were automatically excluded from detailed review by the ICP. The sensitivity of the method was 91\%. The time spent per hospital-wide CAPPS was {\textasciitilde}3 hours. External validation showed a negative predictive value of 99.1\% for CAPPS. CONCLUSIONS CAPPS proved to be a sensitive, accurate, and efficient method to determine serial weekly point-prevalence HAI rates in our hospital. Infect Control Hosp Epidemiol 2016;1-6.},
  keywords = {PRQ1_evaluation_studies_2015+},
  note = {Emma Brown (2023-09-22 23:27:24)(Screen): Not sure if this should be seen as an evaluation of a surveillance system or a method.;
\par
Emma Brown (2023-09-22 23:27:24)(Screen): Not sure if this should be seen as an evaluation of a surveillance system or a method.;}
}
