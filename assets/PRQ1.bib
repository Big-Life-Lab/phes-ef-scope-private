@article{Beebeejaun_Eid_2021,
  title = {Evaluation of {{National Event-Based Surveillance}}, {{Nigeria}}, 2016-2018.},
  author = {Beebeejaun, Kazim and Elston, James and Oliver, Isabel and Ihueze, Adachioma and Ukenedo, Chika and Aruna, Olusola and Makava, Favour and Obiefuna, Ejezie and Eteng, Womi and Niyang, Mercy and Okereke, Ebere and Gobir, Bola and Ilori, Elsie and Ojo, Olubunmi and Ihekweazu, Chikwe},
  year = {2021},
  journal = {Emerging infectious diseases},
  volume = {27},
  number = {3},
  pages = {694--702},
  address = {United States},
  doi = {10.3201/eid2703.200141},
  abstract = {Nigeria Centres for Disease Control and Prevention established an event-based surveillance (EBS) system in 2016 to supplement traditional surveillance structures. The EBS system is comprised of an internet-based data mining tool and a call center. To evaluate the EBS system for usefulness, simplicity, acceptability, timeliness, and data quality, we performed a descriptive analysis of signals received during September 2017-June 2018. We used questionnaires, semistructured interviews, and direct observation to collect information from EBS staff. Amongst 43,631 raw signals detected, 138 (0.3\%) were escalated; 63 (46\%) of those were verified as events, including 25 Lassa fever outbreaks and 13 cholera outbreaks. Interviewees provided multiple examples of earlier outbreak detections but suggested notifications and logging could be improved to ensure action. EBS proved effective in detecting outbreaks, but we noted clear opportunities for efficiency gains. We recommend improving signal logging, standardizing processes, and revising outputs to ensure appropriate public health action.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/40934/Beebeejaun et al. - 2021 - Evaluation of National Event-Based Surveillance, N.pdf}
}
