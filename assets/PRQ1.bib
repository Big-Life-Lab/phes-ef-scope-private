@article{Albali_JFR_2023,
  title = {Comparative Performance Evaluation of the Public Health Surveillance Systems in 6 Gulf Cooperation Countries: Cross-Sectional Study},
  shorttitle = {Comparative Performance Evaluation of the Public Health Surveillance Systems in 6 Gulf Cooperation Countries},
