@article{Hoang_AJoPH_2022,
  title = {Evaluating Epidemic Preparedness and Response to {{COVID-19}}: A Nation-Wide Multisectorial Intra-Action Review of the Points of Entry Surveillance System in {{Vietnam}}, {{January}} to {{September}} 2020.},
  author = {van Hoang, N. and {Hoang NgocAnh} and {Quang ThaiPham} and {Quach HaLinh} and {Nguyen KhanhCong} and {Vu LongNgoc} and {Hoang DucMinh} and Field, E. and Vogt, F.},
  year = {2022},
  journal = {Asia-Pacific Journal of Public Health},
  volume = {34},
  number = {6/7},
  pages = {702--704},
  publisher = {Sage Publications Ltd},
  address = {London, UK},
  doi = {10.1177/10105395221112863},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20220406221&site=ehost-live},
  abstract = {In accordance with the WHO IAR structure, this study sought to identify the advantages and disadvantages of the PoE system in order to offer specific recommendations for enhancing COVID-19 surveillance at the PoE level, both in Vietnam and in other countries in the region. The following themes were separated into five topical FGDs to get participants' opinions on the merits, flaws, and recommendations of the NSSC at PoE: (1) health declaration policies; (2) tools; (3) persons and commodities quarantine policies; (4) interagency cooperation; and (5) laboratory management systems. Overall, there was agreement among workshop participants that the NSSC operated efficiently and effectively at the PoE level. The system was deemed appropriate for the task at hand and capable of quick adjustment to the rapidly changing COVID-19 environment. While the majority of participants felt that the system was sufficient to identify and control low- and medium-level case numbers as well as any associated local outbreaks, its ability to handle high incidence over extended periods of time was questioned as this may have left the system as a whole overworked and understaffed. This was the first IAR to assess a national monitoring system during the COVID-19 pandemic using the WHO's COVID-specific methodology. Overall, it was determined that the COVID-19 surveillance system at PoE level in Vietnam was successful in preventing the importation of numerous cases in 2020. However, a long period of high incidence may have just put too much strain on the system. Lessons learned included the necessity of strengthening emergency preparation, improving procurement procedures, and establishing clear leadership early. Given the lengthy and unpredictable nature of the COVID-19 pandemic, they advise all nations to think about periodically evaluating their monitoring system at the PoE level using suitable techniques as the WHO IAR tool.},
  keywords = {PRQ1_evaluation_studies_2015+}
}

@article{Hoekstra_BotWHO_2000,
  title = {Excluding Polio in Areas of Inadequate Surveillance in the Final Stages of Eradication in {{China}}.},
  author = {Hoekstra, E J and Chai, F and Wang, X J and Zhang, X L and Yu, J J and Bilous, J},
  year = {2000},
  journal = {Bulletin of the World Health Organization},
  volume = {78},
  number = {3},
  pages = {315--20},
  address = {Switzerland},
  abstract = {In 1996, China adopted a virological classification of acute flaccid paralysis (AFP) cases for its surveillance system. Only AFP cases with wild poliovirus in stool specimens are confirmed as polio. Cases with adequate stool specimens that are negative for wild poliovirus are not counted. This paper describes a methodology to rule out poliomyelitis in AFP cases with inadequate stool specimens. National surveillance data were analysed using dot maps to detect clusters of AFP cases with high-risk factors for poliomyelitis. The surveillance system and vaccine coverage were assessed during field investigations. Four clusters of AFP cases were identified, but no poliomyelitis cases. Programmatic failures in the identified high-risk areas included low vaccination rates, poor stool specimen collection and inadequate AFP surveillance. Programme strategies were implemented to correct the identified failures. Use of this methodology provides strong evidence consistent with the absence of wild poliovirus in China.},
  keywords = {No DOI found}
}

@article{HoH._AotAoMS_2016,
  title = {Evaluation of the Staff Sickness Surveillance System in an Acute Care General Hospital in {{Singapore}}},
  author = {{Ho H.} and {Kyaw W.M.} and {Chow A.}},
  year = {2016},
  journal = {Annals of the Academy of Medicine Singapore},
  volume = {45},
  number = {9 Supplement 1},
  pages = {S231},
  publisher = {Academy of Medicine Singapore},
  address = {Netherlands},
  url = {http://www.annals.edu.sg/pdf/45VolNo9Sep2016/SHBC2016.pdf},
  abstract = {Background \& Hypothesis: Staff sickness surveillance systems may be used for early detection of nosocomial outbreaks. Our hospital uses real-time disease surveillance systems (based on daily online reporting of staff medical leave details) with alerts from the Infection Control Unit for early detection of staff infectious disease clusters. We evaluate our current system's performance in detecting acute respiratory infection (ARI) clusters. Method(s): All staff sicknesses due to ARIs and ARI clusters from January 2014 to December 2015 were reviewed, and current thresholds for ARI cluster detection evaluated. To assess data completeness, we compared our data with official records submitted to the hospital's human resource (HR) department. Result(s): The overall completeness of the surveillance data was 77\%. Over the study period, 20\% of ARI medical records were submitted 1-2 weeks after disease occurrence. Hence, only 62\% of ARI medical records were available for real-time surveillance. Using our current cluster detection thresholds of {$>$}=3 ARI cases for small wards and {$>$}=6 cases for large wards, 128 ARI clusters had occurred between January 2014 and December 2015. Of these, 32 were clusters that progressed and required interventions to reduce further transmission. Our system detected 14 such clusters, giving a sensitivity of 43.8\% and a specificity of 79.2\%. The positive predictive value was 41.2\%. Discussion \& Conclusion(s): Our surveillance system is useful for identifying staff ARI clusters for early intervention. Further enhancements to improve cluster detection include integrating the system with the hospital's HR database, providing mobile app platforms to facilitate reporting, and regularly reminding staff about timely and accurate reporting.},
  keywords = {PRQ1_evaluation_studies_2015+},
  file = {files/37792/Ho H. et al. - 2016 - Evaluation of the staff sickness surveillance syst.pdf}
}

