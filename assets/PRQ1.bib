}

@article{Drewe_TED_2015,
  title = {{{SERVAL}}: {{A New Framework}} for the {{Evaluation}} of {{Animal Health Surveillance}}},
  shorttitle = {{{SERVAL}}},
  author = {Drewe, J. A. and Hoinville, L. J. and Cook, A. J. C. and Floyd, T. and Gunn, G. and St{\"a}rk, K. D. C.},
  year = {2015},
