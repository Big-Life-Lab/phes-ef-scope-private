@article{VilgesdeOliveira_Ijoid:I:opotISfID_2018,
  title = {Timeliness in the Notification of Spotted Fever in {{Brazil}}: {{Evaluating}} Compulsory Reporting Strategies and Digital Disease Detection.},
  author = {{Vilges de Oliveira}, Stefan and Nogueira Angerami, Rodrigo},
  year = {2018},
