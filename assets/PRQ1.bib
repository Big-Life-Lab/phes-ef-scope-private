  year = {2010},
  journal = {Chinese Journal of Natural Medicine},
  volume = {12},
  number = {6},
  pages = {443--445},
  publisher = {Publishing House of Chinese Preventive Medicine Association},
  address = {Beijing, China},
  url = {https://search.ebscohost.com/login.aspx?direct=true&db=lhh&AN=20113069899&site=ehost-live},
  abstract = {Objective: To evaluate the quality of network report information on infectious diseases in Lanzhou from 2004 to 2009, and to provide evidence for the improvement of information quality. Methods: The surveillance data were obtained from the Disease Surveillance Information Report Management System. SPSS 11.5 was used for a comprehensive evaluation of the information quality. Results: From 2004 to 2009, a total of 164 973 report cards on notifiable infectious diseases were reported, including 29 142 overdue report cards and 5 424 overdue verification cards. No duplicated cards and report-omitted counties were found. The overdue report rate was 17.66\%, and overdue verification rate 3.29\%. The interval between diagnosis and report of infectious case was 0.56 d, and the interval between report and verification of infectious case was 0.84 h. The report quality was increasing year by year. Higher report quality was observed in Chengguan district, Yuzhong county, and lower quality in Yongdeng county, Xigu district. Conclusion: The quality of infectious disease reporting increased from 2004 to 2009 in Lanzhou. However, regional imbalances of reporting still exists. Overdue report by medical institutions has exerted a direct impact on the performance of the direct network report in Lanzhou. It is necessary to further strengthen the management, guidance and supervision of the direct net work report.},
  keywords = {No DOI found}
}

@article{Watkins_Bph_2006,
  title = {Approaches to the Evaluation of Outbreak Detection Methods.},
  author = {Watkins, Rochelle E and Eagleson, Serryn and Hall, Robert G and Dailey, Lynne and Plant, Aileen J},
